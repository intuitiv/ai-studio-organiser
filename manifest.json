{
  "manifest_version": 3,
  "name": "AI Studio Organizer",
  "version": "1.1",
  "description": "Adds functionality to group, label, and minimize conversations in Google AI Studio.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://aistudio.google.com/*"
      ],
      "js": [
        "organizer.js"
      ],
      "css": [
        "organizer.css"
      ]
    }
  ]
}